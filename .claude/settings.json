{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "env": {
    "OTEL_RESOURCE_ATTRIBUTES": "organization=jadecli,project=jadecli-team-agents-sdk,stack=claude-sdk-mlflow"
  },
  "permissions": {
    "allow": [
      "Bash(mlflow *)",
      "Bash(uvicorn *)",
      "Bash(fastapi *)",
      "Bash(alembic *)",
      "Bash(redis-cli *)",
      "WebFetch(domain:mlflow.org)",
      "WebFetch(domain:docs.mlflow.org)",
      "mcp__redis__*"
    ],
    "additionalDirectories": [
      "/home/org-jadecli/projects/.claude-org",
      "/home/org-jadecli/.claude"
    ]
  },
  "_inherits": "~/projects/.claude-org/settings.json",
  "_project": "jadecli-team-agents-sdk",
  "_description": "Opus 4.6 team agent system using Claude Agent SDK + MLflow 3.9",
  "enabledPlugins": {
    "claude-code-setup@claude-plugins-official": true
  },
  "hooks": {
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "python -c \"from mlflow.claude_code.hooks import stop_hook_handler; stop_hook_handler()\""
          }
        ]
      }
    ]
  },
  "environment": {
    "MLFLOW_CLAUDE_TRACING_ENABLED": "true"
  }
}